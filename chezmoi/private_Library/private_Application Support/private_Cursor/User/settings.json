{
  "[jsx]": {
    "editor.autoClosingBrackets": "never"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "cmake.configureOnOpen": true,
  "cmake.showOptionsMovedNotification": false,
  "console-ninja.featureSet": "Community",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.fontFamily": "TX-02",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "git.autoRepositoryDetection": false,
     "terminal.integrated.autoReplies": {},

  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 9,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "files.autoSave": "onWindowChange",
  "git.enabled": false,
  "git.openRepositoryInParentFolders": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemaDownload.enable": true,
  "jupyter.askForKernelRestart": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "redhat.telemetry.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.env.osx": {
    "HISTFILE": "$HOME/.zsh_history"
  },
  "window.autoDetectColorScheme": true,
  "window.commandCenter": false,
  "workbench.colorTheme": "Default Light+",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "zenMode.fullScreen": false,

  "makefile.configureOnOpen": true,
  "security.promptForRemoteFileProtocolHandling": false,
  "search.useParentIgnoreFiles": true,
  "lldb.library": "/Applications/Xcode-16.2.0.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "lldb.launch.expressions": "native",
  "cursor.composer.collapsePaneInputBoxPills": true,
  "workbench.preferredLightColorTheme": "Default Light Modern",
  "window.customTitleBarVisibility": "never",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vim.useSystemClipboard": true,
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "editor.fontSize": 13,
  "window.confirmSaveUntitledWorkspace": false,
  "cursor.cpp.enablePartialAccepts": true,
  "workbench.preferredDarkColorTheme": "Default Dark Modern",
  "[swift]": {
    "editor.defaultFormatter": "swiftlang.swift-vscode"
  },
  "cursor.composer.cmdPFilePicker2": true,
  "amp.url": "https://ampcode.com/",
  "biome.suggestInstallingGlobally": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "git.enableStatusBarSync": false,
  "cursor.diffs.useCharacterLevelDiffs": true,
  "zig.zls.enabled": "on"
}
